.photo-list {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.photo-list image {
  height: 240rpx;
  width: 240rpx;
}

.item {
  padding: 0 10px;
  margin: 5px 0;
  background: #fff
}

.item-footer {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: baseline;
}

.price {
  color:#1173bc;
  font-size: 1.4em;
}

.area {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.time {
  color: #ccc;
}