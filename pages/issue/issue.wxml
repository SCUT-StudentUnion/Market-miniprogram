<view class="weui-panel weui-panel_access">
  <view class="weui-panel__hd">我的发布</view>
  <view class="weui-panel__bd">
    <navigator wx:for="{{goods_list}}" url="/pages/detail/detail?id={{item.id}}" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
      <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
        <image class="weui-media-box__thumb" src="{{item.pic}}" />
      </view>
      <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
        <view class="weui-media-box__title">{{item.status?"[已通过]":"[未过审]"}}{{item.title}}</view>
        <view class="weui-media-box__desc">{{item.description}}</view>
      </view>
    </navigator>
  </view>

</view>


  <i-collapse name="{{name}}">
    <i-collapse-item wx:for="{{goods_list}}" title="{{item.status?'[已通过]':'[未过审]'}}{{item.title}}" category="电子设备" name="name1" i-class="issue_title">
      <view slot="content" >
      <image class="issue_img" src="{{item.pic}}" />
      <view style='display:block'>  {{item.description}}</view>
      
      </view>
    </i-collapse-item>
    </i-collapse>
